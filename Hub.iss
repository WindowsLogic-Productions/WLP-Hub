Name: "{group}\{cm:ProgramOnTheWeb,Hub ESR}"; Filename: "https://github.com/windowslogic/WLP-Hub"
Name: "{group}\{cm:UninstallProgram,Hub ESR}"; Filename: "{uninstallexe}"
Name: "{commondesktop}\WLP Hub"; Filename: "{app}\WLP Hub.exe"; Tasks: desktopicon

