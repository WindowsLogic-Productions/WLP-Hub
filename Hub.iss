[Files]
Source: "C:\Users\Tim\Desktop\WLP Hub\WLP Hub.exe"; DestDir: "{app}"; Flags: ignoreversion
Source: "C:\Users\Tim\Desktop\WLP Hub\*"; DestDir: "{app}"; Flags: ignoreversion recursesubdirs createallsubdirs
; NOTE: Don't use "Flags: ignoreversion" on any shared system files

[Icons]
